<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>net6.0-windows</TargetFramework>
    <Nullable>enable</Nullable>
    <UseWPF>true</UseWPF>
    <ApplicationIcon>converter_128.ico</ApplicationIcon>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="converter_128.ico" />
  </ItemGroup>

  <ItemGroup>
    <Content Include="converter_128.ico" />
  </ItemGroup>

  <ItemGroup>
    <EmbeddedResource Include="converter_128.ico" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="System.Drawing.Common" Version="6.0.0" />
  </ItemGroup>

</Project>
